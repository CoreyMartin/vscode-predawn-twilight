		"editor.findMatchBackground": "#ffffff0f",
		"editor.findMatchHighlightBackground": "#c6411933",
		"editor.selectionBackground": "#ffffff0f",
		"editor.selectionHighlightBackground": "#ffffff0f",
		"editor.wordHighlightBackground": "#ffffff0f",
		"editor.wordHighlightStrongBackground": "#ffffff0f",
		"editor.inactiveSelectionBackground": "#3C3C3C",
		"editorIndentGuide.background": "#F1826025",
